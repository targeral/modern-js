{
  "name": "@modern-js/core",
  "description": "A Progressive React Framework for modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/modern.js/issues",
  "repository": "modern-js-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "2.3.0",
  "jsnext:source": "./src/index.ts",
  "types": "./src/index.ts",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "jsnext:source": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./bin": {
      "types": "./src/bin.ts",
      "jsnext:source": "./src/bin.ts",
      "default": "./dist/bin.js"
    },
    "./runBin": {
      "types": "./src/runBin.ts",
      "jsnext:source": "./src/runBin.ts",
      "default": "./dist/runBin.js"
    },
    "./types": {
      "types": "./src/types/index.ts",
      "jsnext:source": "./src/types/index.ts",
      "default": "./dist/types/index.js"
    },
    "./config": {
      "types": "./src/types/config/index.ts",
      "jsnext:source": "./src/types/config/index.ts",
      "default": "./dist/types/config/index.js"
    }
  },
  "scripts": {
    "prepublishOnly": "only-allow-pnpm",
    "new": "modern-lib new",
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest"
  },
  "dependencies": {
    "@modern-js/node-bundle-require": "workspace:*",
    "@modern-js/plugin": "workspace:*",
    "@modern-js/utils": "workspace:*"
  },
  "devDependencies": {
    "@jest/types": "^27.0.6",
    "@modern-js/babel-preset-app": "workspace:*",
    "@modern-js/builder-shared": "workspace:*",
    "@modern-js/build-config": "workspace:*",
    "@modern-js/types": "workspace:*",
    "@scripts/build": "workspace:*",
    "@scripts/jest-config": "workspace:*",
    "@types/babel__code-frame": "^7.0.3",
    "@types/babel__core": "^7.1.16",
    "@types/jest": "^27",
    "@types/less": "^3.0.3",
    "@types/node": "^14",
    "autoprefixer": "^10.3.1",
    "btsm": "2.2.2",
    "html-webpack-plugin": "5.5.0",
    "jest": "^27",
    "postcss": "8.4.21",
    "sass": "^1.45.0",
    "terser-webpack-plugin": "^5.1.4",
    "typescript": "^4",
    "webpack": "^5.75.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "jsnext:source": "./src/index.ts",
        "default": "./dist/index.js"
      },
      "./bin": {
        "types": "./dist/bin.d.ts",
        "jsnext:source": "./src/bin.ts",
        "default": "./dist/bin.js"
      },
      "./runBin": {
        "types": "./dist/runBin.d.ts",
        "jsnext:source": "./src/runBin.ts",
        "default": "./dist/runBin.js"
      },
      "./types": {
        "types": "./dist/types/index.d.ts",
        "jsnext:source": "./src/types/index.ts",
        "default": "./dist/types/index.js"
      },
      "./config": {
        "types": "./dist/types/config/index.d.ts",
        "jsnext:source": "./src/types/config/index.ts",
        "default": "./dist/types/config/index.js"
      }
    }
  }
}
